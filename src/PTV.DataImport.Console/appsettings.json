{
    "Data": {
        "AWS": {
            "S3": {
                "AccessKey": "%S3_ACCESS_KEY%",
                "SecretKey": "%S3_SECRET_KEY%",
                "ProfileName": "storage-local",
                "BucketName": "ptv-storage-console-local",
                "Region": "eu-west-1"
            },
            "Log": {
                "AccessKey": "%LOG_ACCESS_KEY%",
                "SecretKey": "%LOG_SECRET_KEY%",
                "ProfileName": "aws-logs",
                "Region": "eu-west-1"
            }
        },
        "DefaultConnection": {
            "ConnectionString": "[REMOVED]",
//            "ConnectionString": "[REMOVED]",
            "StsConnectionString": "[REMOVED]"
        },
        "MapService": {
            "Uri": "https://ws.nls.fi/maasto/wfs",
            "Schema": "?SERVICE=WFS&VERSION=1.1.0&REQUEST=DescribeFeatureType&TYPENAME=oso:Osoitenimi&NAMESPACE=xmlns(oso=http://xml.nls.fi/Osoitteet/Osoitepiste/2011/02)",
            "UserName": "%MAP_USERNAME%",
            "Password": "%MAP_PASSWORD%"
        }
    },
    "PostalCodeCoordinatesSettings": {
        "BatchUrl": "{0}maxFeatures={1}&startIndex={2}",
        "SingleUrl": "{0}CQL_FILTER=posti_alue='{1}'",
        "UrlBase": "http://geo.stat.fi/geoserver/wfs?request=GetFeature&version=1.1.0&typeName=postialue:pno_meri&",
        "BatchSize": 100,
        "DownloadOneByOne": false
    },
    "LogSavedEntities": "true",
    "MaintenanceDatabaseConfiguration": {
        "CommandsPath": "/usr/bin",
        "DumpSchema": "public",
        "DumpName": "PTVbaseGeneral",
        "UpdatedOrNewData": { 
            "RestoreDumpFolderName": "Dumps",       //Folder should be linked from PTV.ExternalSources 
            "BackupDumpFolderName": "DumpsBackup"   //Folder need to create in Docker
        },
        "PreviousData": {
            "RestoreDumpFolderName": "DumpsBackup", //Folder need to create in Docker
            "BackupDumpFolderName": "DumpsBackup"   //Folder need to create in Docker
        },
        "OcpDeployment": {
            "RestoreDumpFolderName": "Dumps",        
            "BackupDumpFolderName": "Dumps"        
        }
    },
    "HolidayConfiguration": {
        "GenerateForRequiredNumberOfYears": 3
    },
    "LogArchiveThreshold": 20
}
