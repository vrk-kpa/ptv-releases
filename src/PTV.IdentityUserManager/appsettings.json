{
    "Data": {
      "AWS": {
        "S3": {
          "AccessKey": "%S3_ACCESS_KEY%",
          "SecretKey": "%S3_SECRET_KEY%",
          "ProfileName": "storage-local",
          "BucketName": "ptv-storage-sts-local",
          "Region": "eu-west-1"
        }
      },
      "DataContextOptions": {
        "RetriesOnDeadlock": "5"
      },
      "DefaultConnection": {
        "ConnectionString": "U[REMOVED]",
        "StsConnectionString": "[REMOVED]"
      }
    },
    "EnvironmentType": "Dev",
    "Logging": {
      "IncludeScopes": false,
      "LogLevel": {
        "Default": "Warning"
      }
    },
  "LogArchiveThreshold": 20
  }
