@model string
<div class="logout-page">
    <div class="page-header">
        <h1>Käyttö estetty</h1>
    </div>

    <div class="row">
        <div class="col-sm-6">
            <p>Tunnuksellasi ei ole oikeutta suorittaa toimintoa.</p>
            @if (!string.IsNullOrEmpty(Model))
            {
                <p>@Model</p>
            }
        </div>
    </div>
</div>