{
  "printWidth": 140,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "endOfLine": "auto",
  "importOrder": [
    "^(react|react-(.*))$",
    "<THIRD_PARTY_MODULES>",
    "^types/(.*)$",
    "^(components|context|fields|hooks|images|mappers|reducers|styles|utils|validation)/(.*)$",
    "^features/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true
}
