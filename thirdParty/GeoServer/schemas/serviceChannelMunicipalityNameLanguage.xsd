<?xml version="1.0" encoding="utf-8"?>
<xsd:schema
	targetNamespace="http://palvelutietovaranto.suomi.fi/ptv"
	xmlns:ptv="http://palvelutietovaranto.suomi.fi/ptv"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:gml="http://www.opengis.net/gml"
	attributeFormDefault="unqualified"
	elementFormDefault="qualified"
	version="1.0">
	
	<xsd:import namespace="http://www.opengis.net/gml" schemaLocation="http://schemas.opengis.net/gml/3.1.1/base/gml.xsd"/>
	
	<xsd:element name="kuntaNimiKielet" type="ptv:KuntaNimiKieletType"  />

	<xsd:complexType name="KuntaNimiKieletType">
		<xsd:complexContent>
			<xsd:extension base="gml:AbstractFeatureType">
				<xsd:sequence>
					<xsd:element name="code" type="xsd:string" />
					<xsd:element name="nimiSuomi" type="xsd:string" />
					<xsd:element name="nimiRuotsi" type="xsd:string" />
					<xsd:element name="nimiEnglanti" type="xsd:string" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
</xsd:schema>