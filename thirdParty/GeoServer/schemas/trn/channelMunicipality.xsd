<?xml version="1.0" encoding="utf-8"?>
<xsd:schema
	targetNamespace="http://palvelutietovaranto.suomi.fi/ptv"
	xmlns:ptv="http://palvelutietovaranto.suomi.fi/ptv"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:gml="http://www.opengis.net/gml"
	attributeFormDefault="unqualified"
	elementFormDefault="qualified"
	version="1.0">
	
	<xsd:import namespace="http://www.opengis.net/gml" schemaLocation="http://schemas.opengis.net/gml/3.1.1/base/gml.xsd"/>
	<xsd:include schemaLocation="https://wfs.palvelutietovaranto.trn.suomi.fi/geoserver/www/schemas/ptv/languageNameType.xsd" />
		
	<xsd:element name="channelMunicipality" type="ptv:ChannelMunicipalityType" />
	
	<xsd:complexType name="ChannelMunicipalityType">
		<xsd:complexContent>
			<xsd:extension base="gml:AbstractGMLType">
				<xsd:sequence>
					<xsd:element name="code" type="xsd:string"/>
					<xsd:element name="municipalityNames" type="ptv:LanguageNameType" minOccurs="1" maxOccurs="unbounded" />
				</xsd:sequence>
				<xsd:attributeGroup ref="gml:AggregationAttributeGroup"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
</xsd:schema>