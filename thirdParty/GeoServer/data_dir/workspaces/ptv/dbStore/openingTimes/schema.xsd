<xsd:schema 
  elementFormDefault="qualified" 
  targetNamespace="http://palvelutietovaranto.suomi.fi/ptv" 
  xmlns:ptv="http://palvelutietovaranto.suomi.fi/ptv"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:gml="http://www.opengis.net/gml">

  <xsd:complexType name="openingTimesType">
    <xsd:complexContent>
      <xsd:extension base="gml:AbstractFeatureType">
        <xsd:sequence>
          <xsd:element maxOccurs="1" minOccurs="0" name="versionedId" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="rootId" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="openingHourId" nillable="false" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="dayFrom" nillable="false" type="xsd:int"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="dayTo" nillable="true" type="xsd:int"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="start" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="end" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="location" nillable="true" type="gml:PointPropertyType"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
</xsd:schema>