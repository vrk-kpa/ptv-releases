<dataStore>
  <id>ptv_geo_datastore</id>
  <name>geoStore</name>
  <description>PTV GEO data store</description>
  <type>PostGIS</type>
  <enabled>true</enabled>
  <workspace>
    <id>ptv_workspace</id>
  </workspace>
  <connectionParameters>
    <entry key="namespace">http://palvelutietovaranto.suomi.fi/ptv</entry>
    <entry key="dbtype">postgis</entry>

    <entry key="host">${ptv.database.host}</entry>
    <entry key="port">${ptv.database.port}</entry>
    <entry key="database">${ptv.database.name}</entry>
    <entry key="user">${ptv.database.user}</entry>
    <entry key="passwd">${ptv.database.password}</entry>
    <entry key="schema">${ptv.database.schema.geo}</entry>

    <entry key="Evictor run periodicity">300</entry>
    <entry key="Max open prepared statements">50</entry>
    <entry key="encode functions">false</entry>
    <entry key="preparedStatements">false</entry>
    <entry key="Batch insert size">1</entry>
    <entry key="Loose bbox">true</entry>
    <entry key="Estimated extends">true</entry>
    <entry key="fetch size">1000</entry>
    <entry key="Expose primary keys">true</entry>
    <entry key="validate connections">true</entry>
    <entry key="Support on the fly geometry simplification">true</entry>
    <entry key="Connection timeout">20</entry>
    <entry key="create database">false</entry>
    <entry key="min connections">1</entry>
    <entry key="max connections">10</entry>
    <entry key="Evictor tests per run">3</entry>
    <entry key="Test while idle">true</entry>
    <entry key="user">${ptv.database.user}</entry>
    <entry key="Max connection idle time">300</entry>
  </connectionParameters>
  <__default>false</__default>
</dataStore>