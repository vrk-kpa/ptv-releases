<xsd:schema 
  elementFormDefault="qualified" 
  targetNamespace="http://palvelutietovaranto.suomi.fi/ptv" 
  xmlns:ptv="http://palvelutietovaranto.suomi.fi/ptv"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:gml="http://www.opengis.net/gml">

  <xsd:complexType name="serviceClassType">
    <xsd:complexContent>
      <xsd:extension base="gml:AbstractFeatureType">
        <xsd:sequence>
          <xsd:element maxOccurs="1" minOccurs="0" name="channelId" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="serviceId" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="channelNameFi" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="channelNameSv" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="channelNameEn" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="serviceNameFi" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="serviceNameSv" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="serviceNameEn" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="serviceClassCode" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="serviceClassDescriptionFi" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="serviceClassDescriptionSv" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="serviceClassDescriptionEn" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="location" nillable="true" type="gml:MultiPointPropertyType"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
</xsd:schema>