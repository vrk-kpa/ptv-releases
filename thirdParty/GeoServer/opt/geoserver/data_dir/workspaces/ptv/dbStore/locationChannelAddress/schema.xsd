<xsd:schema 
  elementFormDefault="qualified" 
  targetNamespace="http://palvelutietovaranto.suomi.fi/ptv" 
  xmlns:ptv="http://palvelutietovaranto.suomi.fi/ptv"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:gml="http://www.opengis.net/gml">

  <xsd:complexType name="locationChannelAddressType">
    <xsd:complexContent>
      <xsd:extension base="gml:AbstractFeatureType">
        <xsd:sequence>
          <xsd:element maxOccurs="1" minOccurs="0" name="addressId" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="versionedId" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="rootId" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="languageCode" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="streetNumber" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="streetName" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="postalCode" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="postalName" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="location" nillable="true" type="gml:PointPropertyType"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
</xsd:schema>